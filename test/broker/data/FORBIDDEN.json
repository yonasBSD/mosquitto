		"tests": [
			{ "name": "00 first packet", "connect": false, "msgs": [{"type":"send", "payload":"00 r0"}]},
			{ "name": "00 remaining length 5 bytes", "msgs":[
				{"type":"send", "payload":"00 r268435456"},
